{
  "bar.customModules.storage.paths": [
    "/"
  ],
  "theme.font.size": "0.8rem",
  "theme.font.weight": 600,
  "bar.layouts": {
    "0": {
      "left": [
        "workspaces"
      ],
      "middle": [
        "clock"
      ],
      "right": [
        "volume",
        "network",
        "bluetooth",
        "battery",
        "systray",
        "notifications"
      ]
    },
    "1": {
      "left": [
        "dashboard",
        "workspaces",
        "windowtitle"
      ],
      "middle": [
        "media"
      ],
      "right": [
        "volume",
        "clock",
        "notifications"
      ]
    },
    "2": {
      "left": [
        "dashboard",
        "workspaces",
        "windowtitle"
      ],
      "middle": [
        "media"
      ],
      "right": [
        "volume",
        "clock",
        "notifications"
      ]
    }
  },
